{
  "name": "simple-md-shell",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://shogotsuneto.github.io/simple-md-shell",
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-syntax-highlighter": "^11.0.2",
    "remark-aozora-ruby": "https://github.com/shogotsuneto/remark-aozora-ruby.git#no-unicode-properties",
    "remark-breaks": "^1.0.3",
    "remark-parse": "^7.0.1",
    "remark-sectionize": "^1.1.0",
    "unified": "^8.3.2"
